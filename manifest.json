{
  "name": "Redmine Enhancer",
  "description": "Modifies and enhances Redmine to make it more useful.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["redmine.js"]
    }
  ],
  "manifest_version": 2
}